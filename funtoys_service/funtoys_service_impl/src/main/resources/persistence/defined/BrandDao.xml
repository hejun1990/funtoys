                and main_flag = #{record.mainFlag}
            </if>
            <if test="record.hql != null">
                and <![CDATA[${record.hql}]]>
            </if>
        </where>
        ORDER BY capital ASC, gmt_created DESC
        <if test="paging != null and paging.start != null and paging.pageSize != null">
